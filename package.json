{
  "name": "traveler-planner",
  "homepage": "http://Grupo-Arthemis.github.io/traveler-planner-site/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.17",
    "@react-google-maps/api": "^2.19.3",
    "concurrently": "^8.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.9",
    "web-vitals": "^2.1.4"
